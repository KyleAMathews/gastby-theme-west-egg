{
  "name": "gatsby-theme-west-egg-mdx-blog",
  "version": "0.0.1",
  "description": "A base mdx blog theme",
  "main": "index.js",
  "license": "MIT",
  "keywords": [
    "gatsby",
    "gatsby-theme",
    "react",
    "blog"
  ],
  "scripts": {
    "develop": "gatsby develop",
    "build": "gatsby build",
    "clean": "gatsby clean",
    "lint": "./node_modules/.bin/eslint \"src/**/*.{ts,tsx,js}\"",
    "eslint-check-rules": "./node_modules/.bin/eslint --print-config . | eslint-config-prettier-check",
    "pretty-check": "prettier --check \"src/**/*.{ts,tsx,js}\"",
    "pretty": "prettier --write \"src/**/*.{ts,tsx,js}\""
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn lint && yarn pretty",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,ts,tsx}": [
      "yarn pretty-check",
      "git add"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-no-null": "^1.0.2",
    "eslint-plugin-prefer-arrow": "^1.1.5",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^1.6.0",
    "gatsby": "^2.4.2",
    "husky": "^2.3.0",
    "lint-staged": "^8.1.7",
    "prettier": "^1.17.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "typescript": "^3.4.5"
  },
  "peerDependencies": {
    "gatsby": "^2.4.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.0.16",
    "@mdx-js/react": "^1.0.16",
    "@mdx-js/tag": "^0.20.3",
    "@types/react": "^16.8.18",
    "@types/react-dom": "^16.8.4",
    "gatsby-mdx": "^0.6.3",
    "gatsby-plugin-compile-es6-packages": "^1.1.0",
    "gatsby-plugin-meta-redirect": "^1.1.1",
    "gatsby-plugin-page-creator": "^2.0.12",
    "gatsby-plugin-redirects": "^1.0.0",
    "gatsby-plugin-typescript": "^2.0.14",
    "gatsby-source-filesystem": "^2.0.33"
  }
}
